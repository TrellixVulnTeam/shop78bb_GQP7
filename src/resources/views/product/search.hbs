 {{#each array}}
  	<form action="/pushstock" method="POST">
    <div class="thumbnail text-center span4 " data-id="{{this._id}}" id="product" >
            <img src="{{this.avatarproduct}}" alt="">
            <div class="caption">
              <h5> Name: {{this.nameproduct}} <span class="badge badge-important">New</span></h5>
			  <p class="label label-inverse"> Giá : {{this.priceproduct}}</p> <p class="label"> $ Giá Khuyên Mãi </p> <br>
              <p class="label label-important"> Số Lượng: {{this.amountproduct}}</p> <p class="label label-warning">Xuất Xứ: Singapore</p>
             <div class="btn-group">
				<span class="btn"><i class="icon-envelope"></i>	<span class="badge badge-inverse">10</span></span>
				<span class="btn"><i class="icon-star"></i>	<span class="badge badge-inverse">10</span></span>
				<span class="btn"><i class=" icon-thumbs-up"></i>	<span class="badge badge-inverse">10</span></span>
					<input type="text" name="id" value="{{this._id}}" class="display" style="display:none">
			  </div>
              <p><a href="/product/{{this.url}}" class="btn btn-primary">Thông tin sản phẩm </a> <input type="submit" class="btn" value="Thêm vào kho"></a></p>
            </div>
			</div>
	
			</form>
  {{/each}}
